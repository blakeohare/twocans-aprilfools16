<build>
	<projectname>TwoCans</projectname>
	<source>source/</source>
	<output>output/%TARGET_NAME%</output>
	
	<spritesheets>
		<sheet id="everything">
			<prefix>*</prefix>
		</sheet>
	</spritesheets>

	<!-- Targets -->
	<target name="js_local">
		<platform>js</platform>
	</target>
	
	<target name="js_remote">
		<platform>js</platform>
		<jsfileprefix>/demos/firedodge/</jsfileprefix>
	</target>
	
	<target name="windows">
		<platform>csopengl</platform>
	</target>
	
	<target name="javaawt">
		<platform>java</platform>
	</target>
	
	<target name="python">
		<platform>python</platform>
	</target>
	
	<target name="android">
		<platform>android</platform>
	</target>
</build>